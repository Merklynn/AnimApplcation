<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.osmobile.capita.animapplcation">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:theme="@style/TheTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NextActivity" />

        <service android:name=".AnimIntentService" />
        <service
            android:name=".AnimService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".AnimService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".TimeAct" />

        <service
            android:name=".TimeService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".ListAct" />
        <activity android:name=".CustomAdap" />
        <activity android:name=".DataBase" />
        <activity android:name=".video" />
        <activity android:name=".Camera"></activity>
    </application>

</manifest>